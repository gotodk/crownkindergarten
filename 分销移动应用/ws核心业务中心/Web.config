<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    注意: 除了手动编辑此文件，还可用 
    Web 管理工具配置应用程序设置。可使用
    Visual Studio 中的“网站”->“Asp.Net 配置”选项。
    所有设置和注释都列在 
    machine.config.comments 中，该文件通常位于 
    \Windows\Microsoft.Net\Framework\vx.x\Config 中
-->
<configuration>
 
  <connectionStrings>
    <!-- 数据库连接 -->
    <add name="mainsqlserver" connectionString="Data Source=db.jwipc.com;Initial Catalog=jwhtml5;Persist Security Info=True;User ID=KD_Mobile;Password=KD@S12_01;Min Pool Size=100;Max Pool Size=5000;Connection Lifetime=50;Connect Timeout=60;Workstation ID=bs" providerName="System.Data.SqlClient" />
    <!-- IPC接口聚合中心配置 , IPCurl是接口中心地址，ThisAppPoolName是本地应用进程池名（部署时清理进程用的） -->
    <add name="IPCurl" connectionString="http://ipc.jwipc.com:28890/fmwscenter.asmx" />
    <add name="ThisAppPoolName" connectionString="吉威医疗-ws核心业务中心" />


    <!-- Redis数据库缓存设置 -->
    <add name="DefaultRedis" connectionString="192.168.10.17|6379" />
  </connectionStrings>
  <appSettings>
    <!-- K3密码对照表 -->
    <add key="K3mimaDZB" value="C:\\Program Files (x86)\\分销移动应用\\ws核心业务中心\\" />
  </appSettings>
  <system.web>
    <!-- 
            设置 compilation debug="true" 可将调试符号插入 
            已编译的页面中。但由于这会 
            影响性能，因此请仅在开发过程中将此值 
            设置为 true。
    -->
    <compilation debug="true" targetFramework="4.0">
      <assemblies>
        <add assembly="System.Numerics, Version=4.0.0.0, Culture=neutral, PublicKeyToken=B77A5C561934E089" />
      </assemblies>
    </compilation>
 
    <pages controlRenderingCompatibilityVersion="4.0" clientIDMode="AutoID" />
    <identity impersonate="false" />
        <sessionState mode="StateServer" />
  </system.web>
  <runtime>
    <!-- 为应用程序禁用 CAS 发行者策略检查 -->
    <generatePublisherEvidence enabled="false" />
  </runtime>
</configuration>